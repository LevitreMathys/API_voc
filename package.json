{
    "name": "api",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "clear && nodemon --exec tsx src/app.ts",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
        "ts:check": "tsc --noEmit",
        "start": "node dist/index.js",
        "test:ui": "vitest --ui",
        "db:start": "docker compose up -d",
        "db:stop": "docker compose down",
        "db:generate": "prisma generate",
        "db:migrate": "prisma migrate dev",
        "db:push": "prisma db push",
        "db:studio": "prisma studio",
        "db:seed": "npm run db:generate && tsx prisma/seed.ts",
        "db:reset": "npm run db:generate && prisma migrate reset --force && npm run db:seed",
        "prepare": "husky"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "dependencies": {
        "@prisma/adapter-pg": "^7.4.1",
        "@prisma/client": "7.4.1",
        "dotenv": "^17.3.1",
        "express": "5.2.1",
        "nodemon": "3.1.14",
        "pg": "^8.19.0"
    },
    "devDependencies": {
        "@types/express": "^5.0.6",
        "@types/node": "^25.3.0",
        "prisma": "7.4.1",
        "ts-node": "^10.9.2",
        "tsx": "4.21.0",
        "typescript": "^5.9.3"
    }
}
